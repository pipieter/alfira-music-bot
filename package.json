    "libsodium-wrappers": "0.7.15",
    "luxon": "3.5.0",
    "typescript": "5.6.3",
    "youtube-search-api": "1.2.2",
    "ytdl-core": "4.11.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
